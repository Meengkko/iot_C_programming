#include <stdio.h>
#include <stdbool.h>
#include <stdlib.h>
#pragma warning (disable:4996)

bool solution(int x) {
	int number;
	int digit=0;
	int digitSum = 0;
	int temp;
	scanf("%d", &number);
	temp = number;
	number = number * 10;
	while (number > 10) {
		digit = number % 10;
		digitSum = digitSum + digit;
		number = number / 10;
	}
	number = temp;
	if (number % digitSum == 0) {
		return true;
	}
	else {
		return false;
	}
}